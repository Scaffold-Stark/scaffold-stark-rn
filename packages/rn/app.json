{
  "expo": {
    "name": "scaffold-stark-rn",
    "slug": "scaffold-stark-rn",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "scaffoldstarkrn",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-font",
      {
        "fonts": [
          {
            "name": "SpaceGrotesk-Regular",
            "file": "./assets/fonts/SpaceGrotesk-Regular.ttf"
          },
          {
            "name": "SpaceGrotesk-Bold",
            "file": "./assets/fonts/SpaceGrotesk-Bold.ttf"
          },
          {
            "name": "SpaceGrotesk-SemiBold",
            "file": "./assets/fonts/SpaceGrotesk-SemiBold.ttf"
          },
          {
            "name": "SpaceGrotesk-Medium",
            "file": "./assets/fonts/SpaceGrotesk-Medium.ttf"
          },
          {
            "name": "SpaceGrotesk-Light",
            "file": "./assets/fonts/SpaceGrotesk-Light.ttf"
          }
        ]
      }
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
